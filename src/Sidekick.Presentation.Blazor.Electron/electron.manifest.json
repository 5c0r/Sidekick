{
  "executable": "Sidekick.Presentation.Blazor.Electron",
  "name": "Sidekick",
  "author": "Sidekick",
  "homepage": "https://github.com/domialex/Sidekick",
  "singleInstance": true,
  "build": {
    "appId": "com.sidekick.app",
    "productName": "Sidekick",
    "buildVersion": "2.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      "**/*"
    ],
    "win": {
      "icon": "../../../Assets/icon.ico"
    }
  }
}
