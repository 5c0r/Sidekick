                    <!--Header-->
                    <Label Grid.Row="0" Grid.Column="0" Content="Level 1" Foreground="{DynamicResource TextColor}" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0" FontWeight="Bold"/>
                    <Label Grid.Row="0" Grid.Column="1" Content="Level 2" Foreground="{DynamicResource TextColor}" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0" FontWeight="Bold"/>
                    <Label Grid.Row="0" Grid.Column="2" Content="Level 3" Foreground="{DynamicResource TextColor}" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0" FontWeight="Bold"/>
                    <Label Grid.Row="0" Grid.Column="3" Content="Contains" Foreground="{DynamicResource TextColor}" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0" FontWeight="Bold"/>
                    <Label Grid.Row="0" Grid.Column="4" Content="Modifiers" Foreground="{DynamicResource TextColor}" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0" FontWeight="Bold"/>
