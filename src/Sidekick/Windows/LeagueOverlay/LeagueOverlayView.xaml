                        <RowDefinition Height="25" Name="rowHeader"/>
                        <RowDefinition Height="30" Name="rowGuardhouse"/>
                        <RowDefinition Height="35" Name="rowWorkshop"/>
                        <RowDefinition Height="35" Name="rowExplosivesRoom"/>
                        <RowDefinition Height="50" Name="rowSplinterResearchLab"/>
                        <RowDefinition Height="30" Name="rowVault"/>
                        <RowDefinition Height="35" Name="rowSparringRoom"/>
                        <RowDefinition Height="35" Name="rowArmourersWorkshop"/>
                        <RowDefinition Height="30" Name="rowJewellersWorkshop"/>
                        <RowDefinition Height="30" Name="rowSurveyorsStudy"/>
                        <RowDefinition Height="35" Name="rowGemcuttersWorkshop"/>
                        <RowDefinition Height="35" Name="rowTormentCells"/>
                        <RowDefinition Height="35" Name="rowStrongboxChamber"/>
                        <RowDefinition Height="50" Name="rowHallOfMettle"/>
                        <RowDefinition Height="50" Name="rowSacrificalChamber"/>
                        <RowDefinition Height="35" Name="rowStorageRoom"/>
                        <RowDefinition Height="30" Name="rowCorruptionChamber"/>
                        <RowDefinition Height="30" Name="rowShrineOfEmpowerment"/>
                        <RowDefinition Height="30" Name="rowTempestGenerator"/>
                        <RowDefinition Height="35" Name="rowPoisionGarden"/>
                        <RowDefinition Height="35" Name="rowTrapWorkshop"/>
                        <RowDefinition Height="35" Name="rowPoolsOfRestoration"/>
                        <RowDefinition Height="30" Name="rowFlameWorkshop"/>
                        <RowDefinition Height="30" Name="rowLightningWorkshop"/>
                        <RowDefinition Height="30" Name="rowHatchery"/>
                        <RowDefinition Height="30" Name="rowRoyalMeetingRom"/>
                        <RowDefinition Height="25" Name="rowIncursionLegend"/>
